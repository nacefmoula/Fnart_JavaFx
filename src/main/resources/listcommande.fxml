<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="800.0" stylesheets="@styles/styles.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.controllers.listcommandeController">
   <children>
      <TableView fx:id="tableView" layoutX="10.0" layoutY="50.0" prefHeight="290.0" prefWidth="780.0">
        <columns>
          <TableColumn fx:id="nomTableColumn" prefWidth="120.0" text="Nom" />
          <TableColumn fx:id="adressTableColumn" prefWidth="140.0" text="Adresse" />
          <TableColumn fx:id="telephoneTableColumn" prefWidth="100.0" text="Téléphone" />
          <TableColumn fx:id="emailTableColumn" prefWidth="140.0" text="Email" />
          <TableColumn fx:id="dateTableColumn" prefWidth="100.0" text="Date" />
          <TableColumn fx:id="totaleTableColumn" prefWidth="100.0" text="Total" />
        </columns>
      </TableView>
      <Text fx:id="listcommande" layoutX="336.0" layoutY="30.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Liste des Commandes" styleClass="title-text" />
      <Button fx:id="btnUpdate" layoutX="605.0" layoutY="355.0" mnemonicParsing="false" text="Modifier" onAction="#btnupdate" styleClass="update-button" />
      <Button fx:id="btnDelete" layoutX="680.0" layoutY="355.0" mnemonicParsing="false" text="Supprimer" onAction="#deletecmd" styleClass="delete-button" />
      <Button fx:id="btnRetour" layoutX="530.0" layoutY="355.0" mnemonicParsing="false" text="Retour" onAction="#retourArtworkList" styleClass="return-button" />
   </children>
</AnchorPane>
